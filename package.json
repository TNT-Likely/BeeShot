{
  "name": "beeshot",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "turbo run dev",
    "dev:playground": "turbo run dev --filter=@beeshot/playground",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "test": "turbo run test",
    "clean": "turbo run clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/node": "^22.10.2",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "shadcn": "^3.6.2",
    "turbo": "^2.3.3",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0"
  },
  "packageManager": "pnpm@9.15.1",
  "engines": {
    "node": ">=20"
  }
}
